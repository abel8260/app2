    "test:integration": {
      "name": "test:integration",
      "command": "npm run test:integration",
      "runAtStart": false
    },
    "test": {
      "name": "test",
      "command": "npm run test",
      "runAtStart": false
    },
    "eslint": {
      "name": "eslint",
      "command": "npm run eslint",
      "runAtStart": false
    },
    "prettier": {
      "name": "prettier",
      "command": "npm run prettier",
      "runAtStart": false
    },
    "format": {
      "name": "format",
      "command": "npm run format",
      "runAtStart": false
    }
  }
